<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form"
	>

        <j:choose>
            <j:when test="${it.isCombinationChecked(p.combination)}">
                <f:checkbox name="${p.combination.toString()}" checked="true"/>
            </j:when>
            <j:otherwise>
                <f:checkbox name="${p.combination.toString()}" checked="false" />
            </j:otherwise>
        </j:choose>					

</j:jelly>